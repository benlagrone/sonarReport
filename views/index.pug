extends layout

block layout-content
  div.View.WelcomeView3
    h1.Banner SonarQube Results
    div.Message
      div.Title
        h3 Code Scan
        h1 Current Code
        ul
          each val in projectsData
              li= val.key + ': '+ val.issues + ', Effort: ' + val.effortTotal/60 + ' hours'

      span.Details Results Breakdown

    include nav.pug